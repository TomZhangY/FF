<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="FileHttpConsumerOperation" class="org.tap.operation.fileconsumer.http.FileHttpConsumerOperation">
		<constructor-arg index="0" ref="JPGConfig"></constructor-arg>
		<constructor-arg index="1" value="${FS.Schedul.FileHttpConsumer.delay}"></constructor-arg>
		<constructor-arg index="2" value="${FS.Schedul.FileHttpConsumer.period}"></constructor-arg>
	</bean>
		

</beans>

